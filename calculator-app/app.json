{
  "expo": {
    "name": "Calculadora rm",
    "slug": "calculadorarm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pablitos_utvt.calculadorarm"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "build": {
      "production": {
        "android": {
          "image": "latest",
          "buildType": "app-bundle"
        },
        "ios": {
          "image": "latest",
          "buildType": "release"
        }
      },
      "preview": {
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": true
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "f2eeb0e6-2af6-4a0c-af02-95a4ca0f3799"
      }
    }
  }
}
